---
import Container from "../../components/Container.astro";

import Post from "../../components/tutorials/Post.astro";
import Sidebar from "../../components/Sidebar.astro";
import Article from "../../components/Article.astro";

import { getCollection } from "astro:content";
import TagList from "../../components/elements/TagList.astro";

const posts = (await getCollection("tutorials")).sort(
    (a, b) => b.data.pubDate.valueOf() - a.data.pubDate.valueOf()
);
---

<Container
    title="Formats"
    description="Documentation for working with interchange file formats"
    image="/banner/format.png"
>
    <Sidebar>
        <TagList
            title="Dash Model Format"
            tags={["Summary", "Python", "Golang"]}
        />
        <TagList title="GLTF" tags={["Summary"]} />
        <TagList title="Noesis" tags={["Summary"]} />
    </Sidebar>

    <Article>
        Dash Model Format Documentation
    </Article>
</Container>
