---
import BaseHead from "../components/BaseHead.astro";
import Navbar from "../components/Navbar.astro";
import Footer from "../components/Footer.astro";

import Nav from "../components/docs/nav.astro";
---

<!DOCTYPE html>
<html lang="en">
    <head>
        <BaseHead
            title="Tools"
            description="Spotlight on Tools"
            css="tailwind"
        />
        <link rel="stylesheet" type="text/css" href="/css/format.css" />
    </head>
    <body class="dark:bg-zinc-900 scroll-smooth dark:[color-scheme:dark]">
        <Nav />
        <main class="grid grid-cols-12 min-h-screen">
            <aside
                id="docs_nav"
                class="max-md:fixed hidden md:block order-1 md:-order-1 col-span-3 bg-zinc-100 dark:bg-zinc-900"
                data-pagefind-ignore
            >
                <div
                    class="max-md:bg-zinc-100 dark:max-md:bg-zinc-900 pt-16 pr-16 w-5/6 md:w-1/4 h-screen fixed overflow-y-auto overflow-x-hidden top-0 border-r-2 border-zinc-200 dark:border-zinc-700"
                >
                    <button
                        id="close"
                        class="md:hidden float-right"
                        aria-label="Close navigation"
                    >
                    </button>
                    <div class="md:float-right w-full max-md:py-4 px-8 md:px-4">
                        <h2
                            class="text-lg font-bold mt-8 py-2 px-4 dark:text-zinc-100"
                        >
                            Get Started
                        </h2><a
                            href="/docs/get-started/introduction"
                            class="relative block py-2 px-4 text-zinc-600 dark:text-zinc-400 hover:text-green-600 dark:hover:text-zinc-300 transition-colors false"
                        >
                            Introduction
                        </a><a
                            href="/docs/get-started/installation"
                            class="relative block py-2 px-4 text-zinc-600 dark:text-zinc-400 hover:text-green-600 dark:hover:text-zinc-300 transition-colors false"
                        >
                            Installation
                        </a><h2
                            class="text-lg font-bold mt-8 py-2 px-4 dark:text-zinc-100"
                        >
                            Tutorials
                        </h2><a
                            href="/docs/tutorials/build-x"
                            class="relative block py-2 px-4 text-zinc-600 dark:text-zinc-400 hover:text-green-600 dark:hover:text-zinc-300 transition-colors false"
                        >
                            Build X With Y
                        </a><h2
                            class="text-lg font-bold mt-8 py-2 px-4 dark:text-zinc-100"
                        >
                            Guides
                        </h2><a
                            href="/docs/guides/migrate-from-z"
                            class="relative block py-2 px-4 text-zinc-600 dark:text-zinc-400 hover:text-green-600 dark:hover:text-zinc-300 transition-colors false"
                        >
